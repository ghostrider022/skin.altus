			<invalidcolor>invalid</invalidcolor>
			<selectedcolor>selected</selectedcolor>
			<texturefocus colordiffuse="$VAR[FocusColorTheme]">colors/grey.png</texturefocus>
			<texturenofocus />
		</definition>
	</include>
	<include name="DialogSettingButton">
		<param name="height">70</param>
		<param name="textoffsetx">40</param>
		<definition>
			<textoffsetx>$PARAM[textoffsetx]</textoffsetx>
			<top>0</top>
			<height>$PARAM[height]</height>
			<aligny>center</aligny>
			<font>font14</font>
			<invalidcolor>invalid</invalidcolor>
			<selectedcolor>selected</selectedcolor>
			<texturefocus colordiffuse="$VAR[FocusColorTheme]">colors/grey.png</texturefocus>
			<texturenofocus colordiffuse="00FFFFFF">colors/white.png</texturenofocus>
		</definition>
	</include>
	<include name="DefaultSettingLabel">
		<left>0</left>
		<top>0</top>
		<height>75</height>
		<label></label>
		<align>center</align>
		<aligny>center</aligny>
		<font>font25_title</font>
		<textcolor>unfocused_text</textcolor>
	</include>
	<include name="SettingsItemCommon">
		<texturenofocus />
		<texturefocus colordiffuse="$VAR[FocusColorTheme]">colors/grey.png</texturefocus>
		<height>70</height>
		<aligny>center</aligny>
		<textoffsetx>20</textoffsetx>
		<radioposy>0</radioposy>
		<focusedcolor>$VAR[FocusedTextColorVar]</focusedcolor>
		<textcolor>unfocused_text</textcolor>
		<alttexturefocus  colordiffuse="$VAR[FocusColorTheme]">colors/grey.png</alttexturefocus>
		<alttexturenofocus />
	</include>
	<include name="SettingsItemCommon1">
		<include>FocusBounce</include>
		<texturefocus colordiffuse="$VAR[FocusColorTheme]">colors/grey.png</texturefocus>
		<texturenofocus colordiffuse="4D000000">colors/dark.png</texturenofocus>
		<height>60</height>
		<aligny>center</aligny>
		<textoffsetx>20</textoffsetx>
		<focusedcolor>$VAR[FocusedTextColorVar]</focusedcolor>
		<textcolor>unfocused_text</textcolor>
		<radioposy>0</radioposy>
		<alttexturefocus  colordiffuse="$VAR[FocusColorTheme]">colors/grey.png</alttexturefocus>
		<alttexturenofocus />
	</include>
	<include name="SettingsLabelCommon">
		<height>50</height>
		<font>font12</font>
		<textoffsetx>30</textoffsetx>
		<textoffsety>10</textoffsety>
		<textcolor>$VAR[FocusColorTheme]</textcolor>
	</include>
	<include name="DefaultDialogButton">
		<param name="width">210</param>
		<param name="height">70</param>
		<param name="wrapmultiline">false</param>
		<param name="font">font27_title</param>
		<param name="onclick"></param>
		<param name="visible">true</param>
		<param name="texture">colors/white.png</param>
		<definition>
			<control type="button" id="$PARAM[id]">
				<include>FocusBounce</include>
				<width>$PARAM[width]</width>
				<height>$PARAM[height]</height>
				<label>$PARAM[label]</label>
				<font>$PARAM[font]</font>
				<textoffsetx>20</textoffsetx>
				<onclick>$PARAM[onclick]</onclick>
				<wrapmultiline>$PARAM[wrapmultiline]</wrapmultiline>
				<align>center</align>
				<aligny>center</aligny>
				<!-- <textoffsety>15</textoffsety> -->
				<textcolor>unfocused_text</textcolor>
				<focusedcolor>$VAR[FocusedTextColorVar]</focusedcolor>
				<texturefocus colordiffuse="$VAR[FocusColorTheme]">$PARAM[texture]</texturefocus>
				<texturenofocus colordiffuse="4D000000">colors/dark.png</texturenofocus>
				<visible>$PARAM[visible]</visible>
			</control>
		</definition>
	</include>
	<include name="KeyboardButton">
    <width>120</width>
    <height>120</height>
    <texturefocus colordiffuse="$VAR[FocusColorTheme]">buttons/roundbutton-fo.png</texturefocus>
    <texturenofocus colordiffuse="4D000000">buttons/roundbutton-fo.png</texturenofocus>
    <textcolor>unfocused_text</textcolor>
    <focusedcolor>$VAR[FocusedTextColorVar]</focusedcolor>
    <align>center</align>
    <aligny>center</aligny>
    <font>font45</font>
	</include>
	<include name="PlaylistWindowButton">
		<param name="width">100%</param>
		<definition>
			<control type="button" id="$PARAM[control_id]">
				<width>$PARAM[width]</width>
				<height>110</height>
				<align>left</align>
				<aligny>top</aligny>
				<texturefocus colordiffuse="$VAR[FocusColorTheme]">colors/white.png</texturefocus>
				<texturenofocus />
				<textoffsetx>40</textoffsetx>
				<textoffsety>35</textoffsety>
				<label>$PARAM[label]</label>
				<onclick>$PARAM[onclick]</onclick>
				<selectedcolor>white</selectedcolor>
			</control>
		</definition>
	</include>
	<include name="IconButton">
    <param name="height">100</param>
    <param name="visible">true</param>
    <definition>
			<control type="radiobutton" id="$PARAM[control_id]">
				<width>100</width>
				<height>$PARAM[height]</height>
				<align>center</align>
				<aligny>center</aligny>
				<onclick>$PARAM[onclick]</onclick>
				<onclick>$PARAM[onclick_2]</onclick>
				<font></font>
				<label>$PARAM[label]</label>
				<texturefocus colordiffuse="$VAR[FocusColorTheme]">buttons/roundbutton-fo.png</texturefocus>
				<texturenofocus/>
				<radioposx>30</radioposx>
				<radioposy>0</radioposy>
				<radiowidth>40</radiowidth>
				<radioheight>40</radioheight>
				<textureradioonfocus colordiffuse="$VAR[FocusedTextColorVar]">$PARAM[icon]</textureradioonfocus>
				<textureradioonnofocus colordiffuse="unfocused_text">$PARAM[icon]</textureradioonnofocus>
				<textureradioofffocus colordiffuse="$VAR[FocusedTextColorVar]">$PARAM[icon]</textureradioofffocus>
				<textureradiooffnofocus colordiffuse="unfocused_text">$PARAM[icon]</textureradiooffnofocus>
				<visible>$PARAM[visible]</visible>
			</control>
    </definition>
	</include>
